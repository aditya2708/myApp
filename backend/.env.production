# PLAN: Production-ready env sample for tenant backend (myApp)
APP_NAME="Kilau Tenant API"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://bp.berbagipendidikan.org

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=kilau_tenant
DB_USERNAME=kilau_tenant
DB_PASSWORD=change-me

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=public
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=localhost
MAIL_PORT=465
MAIL_USERNAME=notif@tenant.kilauindonesia.org
MAIL_PASSWORD=change-me
MAIL_ENCRYPTION=ssl
MAIL_FROM_ADDRESS="notif@tenant.kilauindonesia.org"
